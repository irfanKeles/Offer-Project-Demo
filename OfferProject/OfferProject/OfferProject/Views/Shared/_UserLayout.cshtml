
@{
    Layout = null;
}

<!DOCTYPE html>


<html lang="tr">

<head>
    <meta charset="utf-8">
    <title>Offer System</title>

    <!-- mobile responsive meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- ** Plugins Needed for the Project ** -->
    <!-- plugins -->
    <link rel="stylesheet" href="~/FullTemplate/UserTemplate/theme/plugins/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="~/FullTemplate/UserTemplate/theme/plugins/themify-icons/themify-icons.css">
    <!-- Main Stylesheet -->
    <link href="~/FullTemplate/UserTemplate/theme/css/style.css" rel="stylesheet">
    <!--Favicon-->
    <link href="~/FullTemplate/UserTemplate/fontawsome/css/all.css" rel="stylesheet" />
</head>

<body>

    <header class="sticky-top navigation">
        <div class=container>
            <nav class="navbar navbar-expand-lg navbar-light bg-transparent">
                <a class=navbar-brand href="/userHome/Index"><img class="img-fluid" src="~/FullTemplate/UserTemplate/theme/images/logo.png" alt="godocs"></a>
                <button class="navbar-toggler border-0" type="button" data-toggle="collapse" data-target="#navigation">
                    <i class="ti-align-right h4 text-dark"></i>
                </button>
                <div class="collapse navbar-collapse text-center" id=navigation>
                    <ul class="navbar-nav mx-auto align-items-center">
                        <li class="nav-item"><a class="nav-link" href="/userHome/Index"><i class="fa-solid fa-house"></i> Home</a></li>
                        <li class="nav-item"><a class="nav-link" href="/userHome/GiveOfferPage"> <i class="fa-solid fa-building-circle-arrow-right"></i> Give Offer</a></li>
                        <li class="nav-item"><a class="nav-link" href="/userHome/Profile"><i class="fa-solid fa-id-card"></i> Profile</a></li>
                        <li class="nav-item justify-content-center align-items-center d-flex"><a class="nav-link" href="/userLogin/LogOut"><i class="fa-solid fa-right-from-bracket"></i> Log Out </a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>

    <!-- call to action -->
    <section class="section" style="justify-content:center; align-items:center; display:flex;">
        <div class="container">
            @RenderBody()
        </div>
    </section>
    <!-- /call to action -->
    <!-- plugins -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>

    <!-- my js file-->
    <script src="~/FullTemplate/UserTemplate/includeScript/MyMain.js"></script>

    <!-- /my js file-->
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    @*<script src="~/Scripts/jquery-3.4.1.min.js"></script>*@
    <script src="~/FullTemplate/UserTemplate/theme/plugins/jQuery/jquery.min.js"></script>
    <script src="~/FullTemplate/UserTemplate/theme/plugins/bootstrap/bootstrap.min.js"></script>
    <script src="~/FullTemplate/UserTemplate/theme/plugins/masonry/masonry.min.js"></script>
    <script src="~/FullTemplate/UserTemplate/theme/plugins/clipboard/clipboard.min.js"></script>
    <script src="~/FullTemplate/UserTemplate/theme/plugins/match-height/jquery.matchHeight-min.js"></script>
    <!-- Main Script -->
    <script src="~/FullTemplate/UserTemplate/theme/js/script.js"></script>


</body>
</html>
